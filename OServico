CREATE TABLE ordem_de_servico (
    id INT AUTO_INCREMENT PRIMARY KEY,
    numero_os VARCHAR(20) NOT NULL,
    data DATE NOT NULL,
    hora TIME NOT NULL,
    numero_contrato VARCHAR(50),
    garantia VARCHAR(50),
    eventual_a_pagar VARCHAR(50),
    demonstracao VARCHAR(50),
    numero_ocorrencia VARCHAR(20),
    razao_social VARCHAR(100) NOT NULL,
    endereco VARCHAR(255) NOT NULL,
    cidade VARCHAR(100) NOT NULL,
    cep VARCHAR(10) NOT NULL,
    cnpj VARCHAR(20) NOT NULL,
    telefone_cliente VARCHAR(20) NOT NULL,
    inscricao_estadual VARCHAR(50),
    nome_usuario VARCHAR(100) NOT NULL,
    email_usuario VARCHAR(100) NOT NULL,
    sala_andar VARCHAR(50),
    telefone_usuario VARCHAR(20),
    tipo_equipamento VARCHAR(100) NOT NULL,
    marca VARCHAR(50) NOT NULL,
    modelo VARCHAR(50) NOT NULL,
    serie VARCHAR(50) NOT NULL,
    patrimonio VARCHAR(50) NOT NULL,
    data_atendimento DATE NOT NULL,
    hora_inicio TIME NOT NULL,
    hora_termino TIME NOT NULL,
    serie_correta VARCHAR(50) NOT NULL,
    patrimonio_correto VARCHAR(50) NOT NULL,
    defeito_relatado TEXT NOT NULL,
    defeito_constatado TEXT NOT NULL,
    solucao TEXT NOT NULL,
    estado_equipamento VARCHAR(50) NOT NULL,
    nome_cliente_bkp VARCHAR(100),
    assinatura_cliente_bkp VARCHAR(100),
    tipo_intervencao VARCHAR(50),
    quantidade INT,
    codigo VARCHAR(50),
    descricao VARCHAR(255),
    valor_total DECIMAL(10, 2),
    numero_nf VARCHAR(20),
    total_geral DECIMAL(10, 2),
    numero_horas INT,
    valor_servico_lab DECIMAL(10, 2),
    nome_tecnico VARCHAR(100),
    assinatura_tecnico VARCHAR(255),
    contato_cliente VARCHAR(100),
    assinatura_carimbo_cliente VARCHAR(255)
);
